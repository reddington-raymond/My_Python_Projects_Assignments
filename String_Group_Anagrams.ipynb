      "source": [
        "group= [\"eat\", \"tea\", \"tan\", \"ate\", \"nat\", \"bat\"]\n",
        "g={}\n",
        "for i in group:\n",
        "  s=\"\".join(sorted(i))\n",
        "  if s not in g.keys():\n",
        "    g[s]=[]\n",
        "  g[s].append(i)\n",
        "anagram_list=list(g.values())\n",
        "print(anagram_list)"
      ],
      "execution_count": 4,
