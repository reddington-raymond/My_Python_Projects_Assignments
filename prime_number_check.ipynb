      "source": [
        "num=int(input('Enter a number to be checked in terms of prime number: '))\n",
        "i=2\n",
        "while num>2 and i<int(num/2):\n",
        "  if num%i==0 and i!=num:\n",
        "    print('not a prime number')\n",
        "    break\n",
        "  else:\n",
        "    i+=1\n",
        "else:\n",
        "  print('prime number')"
      ],
