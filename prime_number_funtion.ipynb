      "source": [
        "def prime(n):\n",
        "  prime_list=[2]\n",
        "  for i in range(3,n):\n",
        "    x=2\n",
        "    while x<i:\n",
        "      if i%x==0:\n",
        "        break\n",
        "      else:\n",
        "        x+=1\n",
        "    else:\n",
        "      prime_list.append(i)\n",
        "  return prime_list \n",
        "print(prime(100))"
      ],
